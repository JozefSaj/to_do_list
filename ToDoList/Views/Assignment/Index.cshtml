@model IEnumerable<Assignment>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Assignment</h1>

<table class="table table-bordered table-striped" style="width:100%">
   <thead>
       <tr>
           <th>
               Task
           </th>
           
       </tr>
   </thead>
   <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Name
                </td>
            </tr>
        }
    </tbody>
</table>

